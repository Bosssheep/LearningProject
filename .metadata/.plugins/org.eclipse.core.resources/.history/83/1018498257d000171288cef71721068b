package day18;

import java.util.ArrayList;
import java.util.Collection;

public class IteratorDemo {

	private static void mian(String[] args) {
		/*
		 * 迭代器:	面向接口
		 */
		Collection<String> coll = new ArrayList<String>();
		coll.add("abc1");
		coll.add("abc2");
		coll.add("abc3");
		coll.add("abc4");
		

	}
}
