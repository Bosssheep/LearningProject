public class DateDemo {

	public static void main(String[] args) {
		
		//获取当前日期毫秒值
		//long times = System.currentTimeMillis();
		
		Demo();
	}

	public static void Demo(){
		
		//空参构造，获取到当前操作系统时间
		Date date = new Date();
		//
		Date date1 = new Date(); 
		System.out.println(date1);
	}
}
