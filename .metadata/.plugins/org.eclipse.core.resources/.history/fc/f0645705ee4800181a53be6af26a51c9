package com.MaxSum;
/**
 * 求最大和字串
 * @author HYChan
 *
 */
public class MaxSum {

	
	
	public static int MaxSubArray(int[] arr){
		int n = arr.length;
		int[] End = new int[n];//End[k]表示从arr[0]...arr[k]包含arr[k]的最大的一段数组和
		int[] All = new int[n];//All[k]表示从arr[0]....arr[k]最大的一段数组和
		End[n-1] = arr[n-1];
		All[n-1] = arr[n-1];
		End[0] = All[0] = arr[0];
		
		
		
	}
}
